require 'highline'

puts HighLine.color('!' * 80, :yellow)
puts HighLine.color("The command 'mr' is deprecated.  Use 'murano' instead.", :yellow)
puts HighLine.color('!' * 80, :yellow)

exec 'murano', *ARGV

